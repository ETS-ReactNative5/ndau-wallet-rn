{
  "name": "ndau-wallet-rn",
  "version": "2.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "axios-mock-adapter": "1.16.0",
    "babel-jest": "^26.6.3",
    "chai": "4.2.0",
    "eslint": "7.14.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "2.1.2",
    "jest-serializer-enzyme": "1.0.0",
    "jsdom": "15.1.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "sinon": "7.3.2"
  },
  "scripts": {
    "start": "react-native start",
    "android-clean": "kill $(lsof -t -i:8081); react-native run-android",
    "android": "react-native run-android",
    "android-test-prod": "react-native run-android --variant=release",
    "ios-clean": "kill $(lsof -t -i:8081); react-native run-ios --simulator='iPhone SE'",
    "ios": "react-native run-ios --simulator='iPhone SE'",
    "android-prod": "cd android && ./gradlew clean assembleRelease && cd app/build/outputs/apk/release && zipalign -v -p 4 app-release.apk app-release-signed.apk && apksigner sign --ks ../../../../ndau-wallet-release-key.jks --out app-release.apk app-release-signed.apk",
    "test": "jest",
    "test-ci": "jest --ci --runInBand",
    "testnew": "jest -o",
    "watch": "jest --watch",
    "xcode-open": "open ios/ndauwalletrn.xcodeproj",
    "android-install": "cd android && ./gradlew installDebug",
    "clear-cache": "watchman watch-del-all; rm -rf /tmp/metro-bundler-cache-*; rm -rf /tmp/haste-map-react-native-packager-*",
    "load-assets": "./download_from_s3.sh ndau-fonts 1.0.0",
    "install": "./post_install.sh",
    "postinstall": "npx jetify"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./setupJest.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      ".+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/pro-light-svg-icons": "5.9.0",
    "@fortawesome/pro-regular-svg-icons": "5.9.0",
    "@fortawesome/pro-solid-svg-icons": "5.9.0",
    "axios": "^0.21.1",
    "babel-preset-react-native": "4.0.1",
    "base-64": "0.1.0",
    "crypto-js": "3.1.9-1",
    "events": "3.0.0",
    "jetifier": "^1.6.3",
    "lodash": "^4.17.21",
    "metro-react-native-babel-preset": "0.54.1",
    "moment": "2.24.0",
    "ndaujs": "github:ndau/ndaujs",
    "prop-types": "15.7.2",
    "react-native": "^0.62.0",
    "reactotron-react-native": "^5.0.0"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",

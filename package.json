  "scripts": {
    "start": "react-native start",
    "android-clean": "kill $(lsof -t -i:8081); react-native run-android",
    "android": "react-native run-android",
    "android-test-prod": "react-native run-android --variant=release",
    "ios-clean": "kill $(lsof -t -i:8081); react-native run-ios --simulator='iPhone 8'",
    "ios": "react-native run-ios --simulator='iPhone XS Max'",
    "android-prod": "cd android && ./gradlew clean assembleRelease && cd app/build/outputs/apk/release && zipalign -v -p 4 app-release.apk app-release-signed.apk && apksigner sign --ks ../../../../ndau-wallet-release-key.jks --out app-release.apk app-release-signed.apk",
    "test": "jest",
    "test-ci": "jest --ci --runInBand",
    "testnew": "jest -o",
    "xcode-open": "open ios/ndauwalletrn.xcodeproj",
    "android-install": "cd android && ./gradlew installDebug",
    "clear-cache": "watchman watch-del-all; rm -rf /tmp/metro-bundler-cache-*; rm -rf /tmp/haste-map-react-native-packager-*",
    "load-assets": "./download_from_s3.sh ndau-fonts 1.0.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./setupJest.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-navigation-tabs|react-native-responsive-screen|react-native-vector-icons|react-native-button|react-native-qrcode|react-native-safe-area-view|react-native-tab-view|react-navigation|react-native-check-box|react-native-exit-app|react-native-autocomplete-input|react-native-modal-dropdown|react-native-simple-dialogs|react-native-flash-message|react-native-loading-spinner-overlay)/)"
    ]
  },
  "dependencies": {
    "@react-navigation/core": "^3.0.2",
    "@react-navigation/native": "^3.1.1",
    "axios": "^0.18.0",
    "babel-preset-react-native": "4.0.1",
    "base-64": "0.1.0",
    "crypto-js": "3.1.9-1",
